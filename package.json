{
  "name": "@palastri/eslint-plugin-react",
  "version": "0.0.0",
  "description": "ESLint plugin for React",
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin",
    "react",
    "typescript"
  ],
  "author": "Imam S Rifkan <imamsrifkan@gmail.com>",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/palastri/eslint-plugin-react.git"
  },
  "license": "MIT",
  "scripts": {
    "build": "tsc -b tsconfig.json",
    "lint": "eslint --cache .",
    "prepack": "rimraf dist && yarn build",
    "prettier:check": "prettier --check package.json tsconfig.json '.github/**' .yarnrc.yml",
    "prettier:write": "prettier --write package.json tsconfig.json '.github/**' .yarnrc.yml",
    "typecheck": "tsc -p ."
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.6.0",
    "eslint-plugin-eslint-plugin": "^5.1.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^2.8.8"
  },
  "devDependencies": {
    "@types/node": "^18.16.19",
    "eslint": "^8.44.0",
    "rimraf": "^5.0.1",
    "typescript": "^4.7.2"
  },
  "engines": {
    "node": "^16.0.0 || >= 18.0.0"
  },
  "files": [
    "*.md",
    "LICENSE",
    "dist"
  ],
  "peerDependencies": {
    "eslint": ">=8.0.0"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  },
  "packageManager": "yarn@3.6.3"
}
